{
    "plugins": [
        ["transform-runtime", {
            "polyfill": true,
            "regenerator": true
        }],
        ["transform-es2015-modules-commonjs-simple", {
            "noMangle": true
        }]        
    ],

    "ignore": [
        "node_modules/**"
    ],

    "presets": [
        "es2015",
        "stage-0"
    ]
}